{
    "name": "umbrella/adminbundle",
    "type": "symfony-bundle",
    "description": "Umbrella Admin Symfony bundle.",
    "keywords": [
        "backend",
        "admin",
        "umbrella"
    ],
    "homepage": "https://github.com/acantepie/umbrella",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrien Cantepie",
            "email": "adrien.cantepie@gmail.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": ">=8.2",
        "doctrine/dbal": "^3.9.4",
        "doctrine/doctrine-bundle": "^2.13.2",
        "doctrine/orm": "^3.3.2",
        "symfony/asset": "6.4.*",
        "symfony/console": "6.4.*",
        "symfony/config": "6.4.*",
        "symfony/dependency-injection": "6.4.*",
        "symfony/filesystem": "6.4.*",
        "symfony/form": "6.4.*",
        "symfony/framework-bundle": "6.4.*",
        "symfony/http-foundation": "6.4.*",
        "symfony/http-kernel": "6.4.*",
        "symfony/mailer": "6.4.*",
        "symfony/mime": "6.4.*",
        "symfony/routing": "6.4.*",
        "symfony/security-bundle": "6.4.*",
        "symfony/translation": "6.4.*",
        "symfony/options-resolver": "6.4.*",
        "symfony/twig-bundle": "6.4.*",
        "symfony/yaml": "6.4.*",
        "umbrella2/corebundle": "*",
        "symfony/maker-bundle": "^1.36",
        "stof/doctrine-extensions-bundle": "^1.7"
    },
    "conflict": {
        "umbrella2/corebundle": "<6.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "symfony/browser-kit": "6.4.*",
        "symfony/css-selector": "6.4.*",
        "symfony/phpunit-bridge": "6.4.*"
    },
    "suggest": {
        "knplabs/knp-time-bundle": "Display pretty time on notification"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Umbrella\\AdminBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Umbrella\\AdminBundle\\Tests\\": "tests/",
            "Umbrella\\AdminBundle\\Tests\\TestApp\\": "tests/TestApplication/app/src/"
        }
    }
}
