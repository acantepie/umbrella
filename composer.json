{
    "name": "umbrella2/admin-bundle",
    "type": "symfony-bundle",
    "description": "Umbrella Admin Symfony bundle.",
    "keywords": [
        "backend",
        "admin",
        "umbrella"
    ],
    "homepage": "https://github.com/acantepie/umbrella",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrien Cantepie",
            "email": "adrien.cantepie@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "doctrine/dbal": "^3.9.4",
        "doctrine/doctrine-bundle": "^2.13.2",
        "doctrine/orm": "^3.3.2",
        "knplabs/knp-time-bundle": "^2.4",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/asset": "^6.4|^7.0",
        "symfony/config": "^6.4|^7.0",
        "symfony/console": "^6.4|^7.0",
        "symfony/dependency-injection": "^6.4|^7.0",
        "symfony/event-dispatcher": "^6.4|^7.0",
        "symfony/filesystem": "^6.4|^7.0",
        "symfony/form": "^6.4|^7.0",
        "symfony/framework-bundle": "^6.4|^7.0",
        "symfony/http-foundation": "^6.4|^7.0",
        "symfony/http-kernel": "^6.4|^7.0",
        "symfony/mailer": "^6.4|^7.0",
        "symfony/maker-bundle": "^1.36",
        "symfony/mime": "^6.4|^7.0",
        "symfony/options-resolver": "^6.4|^7.0",
        "symfony/property-access": "^6.4|^7.0",
        "symfony/routing": "^6.4|^7.0",
        "symfony/security-bundle": "^6.4|^7.0",
        "symfony/translation": "^6.4|^7.0",
        "symfony/twig-bundle": "^6.4|^7.0",
        "symfony/validator": "^6.4|^7.0",
        "symfony/yaml": "^6.4|^7.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.4|^7.0",
        "doctrine/doctrine-fixtures-bundle": "^4.0.0",
        "friendsofphp/php-cs-fixer": "^3.70",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "symfony/browser-kit": "^6.4|^7.0",
        "symfony/css-selector": "^6.4|^7.0"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "Umbrella\\AdminBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Umbrella\\AdminBundle\\Tests\\": "tests/",
            "Umbrella\\AdminBundle\\Tests\\App\\": "tests/App/src/"
        }
    }
}
