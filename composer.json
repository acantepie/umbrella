{
    "name": "acantepie/umbrella",
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "doctrine/dbal": "^3.9.4",
        "doctrine/doctrine-bundle": "^2.13.2",
        "doctrine/doctrine-fixtures-bundle": "^4.0.0",
        "doctrine/orm": "^3.3.2",
        "friendsofphp/php-cs-fixer": "^3.70",
        "knplabs/knp-time-bundle": "^2.4",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/asset": "^6.4",
        "symfony/browser-kit": "^6.4",
        "symfony/config": "^6.4",
        "symfony/console": "^6.4",
        "symfony/css-selector": "^6.4",
        "symfony/dependency-injection": "^6.4",
        "symfony/event-dispatcher": "^6.4",
        "symfony/filesystem": "^6.4",
        "symfony/form": "^6.4",
        "symfony/framework-bundle": "^6.4",
        "symfony/http-foundation": "^6.4",
        "symfony/http-kernel": "^6.4",
        "symfony/mailer": "^6.4",
        "symfony/maker-bundle": "^1.36",
        "symfony/mime": "^6.4",
        "symfony/monolog-bundle": "^3.7",
        "symfony/options-resolver": "^6.4",
        "symfony/phpunit-bridge": "^6.4",
        "symfony/property-access": "^6.4",
        "symfony/routing": "^6.4",
        "symfony/security-bundle": "^6.4",
        "symfony/translation": "^6.4",
        "symfony/twig-bundle": "^6.4",
        "symfony/validator": "^6.4",
        "symfony/yaml": "^6.4"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Umbrella\\AdminBundle\\": "Bundle/AdminBundle/src",
            "Umbrella\\AdminBundle\\Tests\\": "Bundle/AdminBundle/tests/",
            "Umbrella\\AdminBundle\\Tests\\App\\": "Bundle/AdminBundle/tests/App/src/",

            "Umbrella\\CoreBundle\\": "Bundle/CoreBundle/src",
            "Umbrella\\CoreBundle\\Tests\\": "Bundle/CoreBundle/tests/",
            "Umbrella\\CoreBundle\\Tests\\App\\": "Bundle/CoreBundle/tests/App/src/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    }
}
